{
  "name": "rerp",
  "version": "1.0.0",
  "description": "a react, redux template that gets you up and running in no time, bootsrapped with ParcelJS",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "parcel src/index.html -p 3000 --out-dir public --open",
    "build": "parcel build src/index.html --out-dir build",
    "delete-build": "rm -rf build .cache dist",
    "format": "prettier --write \"**/*.{js,jsx,scss,css,html}\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Caleb335/rrp.git"
  },
  "bin": {
    "rrp": "index.js"
  },
  "keywords": [
    "react",
    "redux",
    "parcel"
  ],
  "author": "Caleb Olojo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Caleb335/rrp/issues"
  },
  "homepage": "https://github.com/Caleb335/rrp#readme",
  "dependencies": {
    "colors": "^1.4.0",
    "node-sass": "^4.14.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.1",
    "redux": "^4.0.5",
    "shelljs": "^0.8.4"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "parcel-bundler": "^1.12.4",
    "prettier": "^2.1.1"
  }
}
